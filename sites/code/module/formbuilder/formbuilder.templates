@@@formbuilder_content_row@@@
<div data-<%=template%>="<%=_tid%>" style="position:relative;">
	<div class="feeditems3 formbuildercontent <%if(enableRequiredAsterisk){%>formbuildershowasterisk<%}%>" style="background:white;padding-top:5px;padding-bottom:15px">
		Inline Item!
	</div>
</div>
@@@formbuilder_content@@@
<div <%if(!inline){%>class="sfit"<%}%> data-<%=template%>="<%=_tid%>" style="<%if(inline){%>position:relative;<%}%>">
	<div class="feeditems3 formbuildercontent <%if(enableRequiredAsterisk){%>formbuildershowasterisk<%}%> <%if(mobileLayout){%>formbuilder_forcemobilelayout<%}%>" style="background:white;padding-top:5px;padding-bottom:15px">
		<%
			var order=(schema.renderOrder)?schema.renderOrder:schema.order;
		%>
		<%if(schema.guidelines){%>
		<div style="padding:5px;border-bottom:2px solid #ccc;" class="truebox">
			<div style="font-size:14px;font-weight: bold;padding-bottom: 3px;"><%if(schema.guideline_name){%><%=schema.guideline_name%><%}else{%>Posting Guidelines<%}%></div>
			<div style="padding:5px;font-size: 16px;"><%=schema.guidelines%></div>
		</div>
		<%}%>
		<%for (var i = 0; i < order.length; i++) {
			var item=schema.fields[order[i]];
			if(!item){
				console.warn('invalid schema item ['+order[i]+'], not found in field');
				continue;
			}
			item.id=order[i];
			if(!item.form) continue;
			if(excludeFields.indexOf(order[i])>=0) continue;
			if(includeFields&&includeFields.indexOf(order[i])==-1) continue;
			if(item.adminOnly&&!app.user.profile.admin) continue;
			if(item.permission&&!_.intersect(item.permission,app.user.profile.roles).length) continue;
			%>
			<%if(types[item.form.type]){%>
			<div style="position:relative;">
			<%=types[item.form.type].render(order[i],item)%>
			<%if(item.form.privacy){
				item.currentPrivacy=_.dotGet(item.form.privacy.key,current);
			%>
			<div style="position:absolute;top:5px;right:5px"><%=$.fn.render({template:'formbuilder_privacy_'+item.form.privacy.type,returntemplate:true,data:{item:item}})%></div>
			<%if(item.required){%>
			<i class="icon-asterisk" style="position:absolute;top:20px;right:5px;color:#888;font-size:12px"></i>
			<%}%>
			<%}else{%>
			<div style="position:absolute;top:5px;right:5px;" class="notonweb">
			<%if(item.tooltip){%>
			<i class="icon-info-circled-alt x_tooltip" data-id="<%=item.id%>" style="color:#888;font-size:22px;margin-right: 5px;"></i>
			<%}%>
			<%if(item.required&&item.form.type!='hidden'){%>
			<i class="icon-asterisk" style="color:#888;font-size:22px"></i>
			<%}%>
			</div>
			<%}%>
			<%if(item.subText){%>
			<div><%=item.subText%></div>
			<%}%>
			</div>
			<%}else{%>
			<%console.warn('Invalid render type ['+item.form.type+']');%>
			<%}%>
		<%}%>
		<%if(canDelete){%>
		<div style="padding-top:50px;text-align: center;">
			<div style="background: #f02;padding:5px 10px;color:white;display: inline-block;margin:auto" action="click:remove" class="m-corner-all"><i class="icon-trash-empty"></i> Delete Item</div>
		</div>
		<%}%>
		<%if(!inline){%>
		<div style="height:80vh" class="notonweb"></div>
		<div style="height:80px" class="notonmobile"></div>
		<%}%>
	</div>
	<div style="position: fixed;bottom:0;left:0;right:0;display: none;background:white;z-index: 100" class="mainsearchresults"></div>
</div>
@@@formbuilder_privacy_basic@@@
<div style="padding:4px 8px;background:#dedede" class="m-corner-all" action="click:setPrivacy" data-privacy="<%=item.id%>">
	<%if(item.currentPrivacy&&item.currentPrivacy=='private'){%>
	Privacy: <i class="icon-lock"></i>
	<%}else{%>
	Privacy: <i class="icon-globe"></i>
	<%}%>
</div>
@@@formbuilder_select_list@@@
<div data-<%=template%>="<%=_tid%>" style="max-height:200px;" class="scrollY">
	<div>
		<%for (var i = 0; i < data.length; i++) {
			var item=data[i];%>
			<div style="padding:5px;border-bottom:1px solid #ccc" class="formbuilderselectitem reghover x_select <%if(current&&current==item.value){%>selected<%}%>" data-value="<%=item.value%>">
				<table style="width:100%">
					<tr>
						<td style="width:30px;text-align: center">
							<i class="icon-check-1 selectcheck highlighttext2"></i>
						</td>
						<td style="padding-left:5px;font-size:16px;text-align: left;">
							<%=item.display%>
						</td>
					</tr>
				</table>
			</div>
		<%}%>
	</div>
</div>
@@@formbuilder_input_page_web@@@
<div data-<%=template%>="<%=_tid%>" style="position: absolute;z-index: 1000;padding:<%=padding%>px;" class="apppageview">
	<div class="content" style="<%if(this.width){%>width:<%=width%>;<%}else{%>width:420px;<%}%>background: white;border:1px solid #ccc;">
		<%if(!opts.notitle){%>
		<%if(opts.title){%>
		<div style="padding:5px;font-size:18px;font-weight: bold;margin-bottom:0px">
			<table style="width:100%;color:#888">
				<tr>
					<td style="width:50px;text-align: left;padding-left:5px" class="x_closer truebox">
						<i class="icon-cancel" style="font-size:14px;"></i>
					</td>
					<td style="text-align: center;font-size:18px;color:#888;padding:3px;"><%if(opts.title){%><%=opts.title%><%}%></td>
					<td style="width:50px;text-align: right;padding-right:5px;font-weight: bold;font-size:18px;" class="x_set highlightcolor">
						<%if(opts.done){%>
						<%=opts.done%>
						<%}else{%>
						Set
						<%}%>
					</td>
				</tr>
			</table>
		</div>
		<%}else{%>
		<%}%>
		<%}%>
		<div class="inputarea">
		</div>
		<%if(!opts.notitle){%>
		<%if(!opts.title){%>
		<div style="padding:5px;font-size:13px;text-align: center;" class="themebutton x_closer">
			Close
		</div>
		<%}%>
		<%}%>
		<%if(opts.canClear){%>
		<div style="padding:5px;font-size:13px;text-align: center;" class="themebutton x_clear">
			Clear
		</div>
		<%}%>
	</div>
</div>
@@@formbuilder_input_page@@@
<div data-<%=template%>="<%=_tid%>" style="width:100%;height:100%;<%if(fixed){%>position:fixed;<%}else{%>position:absolute;<%}%>top:0;left:0;z-index: 100000;background: transparent;overflow: hidden" class="apppageview">
	<div style="position: absolute;top:0;left:0;width:100%;height:100%;" class="pane">
		<div style="position: absolute;bottom:0;left:0;right:0;background: white;" class="content">
			<div style="position: absolute;bottom:100%;left:0;right:0;margin-bottom:5px;padding:5px;text-align: center;height:160px;" class="swiper">
				<div style="width:80px;height:6px;background:rgba(255,255,255,.6);position: absolute;bottom:0;left:50%;margin-left:-40px;" class="s-corner-all"></div>
			</div>
			<%if(this.title){%>
			<div style="padding:5px;font-size:18px;font-weight: bold;margin-bottom:0px">
				<table style="width:100%;color:#888">
					<tr>
						<td style="width:50px;text-align: left;padding-left:5px" class="x_closer truebox">
							<i class="icon-cancel" style="font-size:14px;"></i>
						</td>
						<td style="text-align: center;font-size:18px;color:#888;padding:3px;"><%=title%></td>
						<td style="width:50px;text-align: right;padding-right:5px;font-weight: bold;font-size:18px;" class="x_set highlightcolor">
							Done
						</td>
					</tr>
				</table>
			</div>
			<%}%>
			<div class="inputarea">
			</div>
			<%if(this.canClear){%>
			<div style="padding:10px;font-size:13px;text-align: center;" class="themebutton x_clear">
				Clear
			</div>
			<%}%>
		</div>
	</div>
</div>
@@@formbuilder_moreinfo@@@
<span>
	<%if(item.required){%>
	<span aria-label="Required" data-balloon-pos="up">
		<sup>
			<i class="highlighttext2 icon-asterisk" style="font-size:7px"></i>
		</sup>
	</span>
	<%}%>
</span>
@@@formbuilder_url_name@@@
