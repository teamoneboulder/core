@@@formbuilder_tooltip@@@
<div data-<%=template%>="<%=_tid%>" style="display: inline-block;">
	<i class="icon-info-circled-alt" data-tooltip="<%=data.tooltip%>" data-tooltip-pos="right" data-tooltip-size="large"></i>
</div>
@@@formbuilder_media@@@
<div class="formelement forminput2" data-type="media" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" <%if(form.group){%>data-group="<%=form.group%>"<%}%>>
	<div class="webplaceholder"><%=name%> <%=$.fn.render({template:'formbuilder_moreinfo',data:{item:this},returntemplate:true})%></div>
	<div class="formbuilder_contentarea">
		<div style="padding:0px 10px">
			<div class="placeholdertitle notonweb" style="padding-bottom:10px;"><%=name%> > <%=$.fn.render({template:'formbuilder_moreinfo',data:{item:this},returntemplate:true})%></div>
			<div class="mediatray">
				<%if(this.current&&current.order){%>
				<%for (var i = 0; i < current.order.length; i++) {
					var item=current.list[current.order[i]];%>
					<%=$.fn.render({template:'formbuilder_previewimg',data:{id:item.id,pic:item.data,editing:false},returnhtml:true})%>
				<%}%>
				<%}%>
			</div>
			<div>
				<div style="display: inline-block;margin:5px;border:1px solid #ccc;padding:5px;" class="s-corner-all x_addphotos">
					<div style="font-size:15px"><i class="icon-images"></i> Add Photos</div>
				</div>
			</div>
		</div>
	</div>
</div>
@@@formbuilder_previewimg@@@
<div data-<%=template%>="<%=_tid%>" style="display:inline-block;margin:5px;position:relative;" data-image="<%=id%>">
	<img src="<%if(this.pic){%><%=_.getImg(pic,'small')%><%}else{%><%=app.idot%><%}%>" style="height:80px" class="s-corner-all previewimg">
	<div style="position:absolute;top:-12px;right:-12px;background:#f00;padding:5px;display: none;" class="circle x_remove" data-id="<%=id%>">
		<div style="width:15px;height:15px;">
			<table style="width:100%;height:100%;text-align: center;"><tr><td>
			<i class="icon-cancel" style="font-size:8px;padding:0;margin:0;color:white;font-weight: bold"></i>
			</td></tr></table>
		</div>
	</div>
	<div style="position:absolute;top:0px;right:0px;padding:6px;color:white;background: rgba(55,55,55,.4);" class="x_remove s-corner-all" data-id="<%=id%>">
		<i class="icon-cancel" style="font-size:12px;padding:0;margin:0;color:white;font-weight: bold"></i>
	</div>
	<%if(editing){%>
	<div class="sfit indicator s-corner-all" style="background: rgba(55,55,55,.3);z-index: 10;bottom:5px"></div>
	<%}%>
</div>
@@@formbuilder_payment@@@
<div class="formelement forminput2" data-type="payment" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" <%if(form.group){%>data-group="<%=form.group%>"<%}%>>
	<div class="webplaceholder"><%=name%> <%=$.fn.render({template:'formbuilder_moreinfo',data:{item:this},returntemplate:true})%></div>
	<div class="formbuilder_contentarea">
		<div style="padding:0px 10px">
			<div class="placeholdertitle notonweb" style="padding-bottom:10px;"><%=name%> <%=$.fn.render({template:'formbuilder_moreinfo',data:{item:this},returntemplate:true})%></div>
			<%if(this.methods&&methods){%>
			<%if(df(methods,'data.usd.methods')){%>
			 <%for (var i = 0; i < methods.data.usd.methods.length; i++) {
                var card=methods.data.usd.methods[i];%>
                <div class="cardmethod toggler <%if(methods.data.usd.default_source&&methods.data.usd.default_source==card.id){%>checked<%}%>" data-id="<%=card.id%>">
					<table style="width:100%">
						<tr>
							<td style="width:40px;text-align: center;" class="">
								<i class="icon-check" style="font-size:30px;color:#0193be"></i>
								<i class="icon-check-empty" style="font-size:30px;"></i>
							</td>
							<td style="width:40px;text-align: center;">
								<i class="icon-credit-card" style="font-size:18px;"></i>
							</td>
							<td style="padding-left:5px;font-size:18px"><%=card.brand%> &middot; <%=card.last4%>
							</td>
							<td style="width:100px;text-align: right;padding-right:5px">
								exp <%=card.exp_month%>/<%=card.exp_year%>
							</td>
						</tr>
					</table>
				</div>
            <%}%>
            <%}else{%>
            <div>No cards added yet</div>
            <%}%>
            <div style="padding:10px;text-align:center;">
            	<div class="button1 l-corner-all x_addcard" style="display: inline-block;font-size:12px;padding:3px 10px"><i class="icon-add-clean"></i> Add a card</div>
            </div>
			<%}else{%>
			<i class="icon-refresh animate-spin"></i> Loading Payment Methods...
			<%}%>
		</div>
	</div>
</div>
@@@formbuilder_onoff@@@
<div class="formelement forminput2" data-type="onoff" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" <%if(form.group){%>data-group="<%=form.group%>"<%}%>>
