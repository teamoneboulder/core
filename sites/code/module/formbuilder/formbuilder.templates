	<div style="padding:10px;font-size:14px"><i class="icon-info-circled-alt"></i> We were not able to validate this url.  If you believe it is correct, press Continue below.</div>
	<div style="padding:10px;margin:10px;text-align: center;font-size:16px;font-weight: bold;" class="x_continue">
		Continue
	</div>
</div>
@@@formbuilder_mulitselect_items2@@@
<div data-<%=template%>="<%=_tid%>">
<%if(max){%>
<div style="padding:10px;text-align: center;">Select up to <span class="bold"><%=max%></span></div>
<%}%>
<div style="max-height:60vh;" class="scrollY">
	<%if(menu&&menu.length){%>
	<%for (var i = 0; i < menu.length; i++) {
		var item=menu[i];
	%>
	<div style="padding:4px;font-size:18px" class="multibuttonitem <%if(current&&current.values.indexOf(item.value)>=0){%>selected<%}%>" data-id="<%=item.value%>">
		<table style="width:100%">
			<tr>
				<td style="width:30px;text-align: center">
					<div style="width:30px;height:30px;border:1px solid #ccc" class="circle">
						<table style="width:100%;height:100%;text-align: center;">
							<tr>
								<td>
									<i class="icon-check-1 highlighttext2 formbuildermultiselect_check fasttransition" style="font-size:18px;"></i>
								</td>
							</tr>
						</table>
					</div>
				</td>
				<td style="padding-left:8px;padding-right:5px;vertical-align: middle;text-align: left;"><b><%=item.display%></b></td>
			</tr>
		</table>
	</div>
