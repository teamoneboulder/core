@@@formbuilder_hidden@@@
<div class="formelement forminput2" data-type="hidden" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" style="display: none;"></div>
@@@formbuilder_timezone@@@
<div class="formelement forminput2" data-type="timezone" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" <%if(form.group){%>data-group="<%=form.group%>"<%}%>>
