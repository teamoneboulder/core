	<div class="formbuilder_contentarea">
		<div style="padding:0px 10px">
			<div class="placeholdertitle notonweb" style="padding-bottom:4px;padding-top:4px"><%=name%></div>
			<div>
				<input class="forminput formbuilder_input" type="text" value="<%=current%>" placeholder="<%=name%>"/>
			</div>
			<div style="padding-left:15px;padding-bottom:10px">
				<%=app.domain%>/<%if(form.prefix){%><%=form.prefix%><%}%><span class="currentvalue"><%=current%></span> <span class="iconarea"></span>
			</div>
			<div class="response"></div>
		</div>
	</div>
</div>
@@@formbuilder_url_response@@@
<div data-<%=template%>="<%=_tid%>" style="padding-left:15px">
	<%if(this.loading){%>
	<div style="" class="bold">
		<i class="icon-re esh animate-spin"></i> Loading...
	</div>
	<%}%>
	<%if(this.error){%>
	<div style="color:green" class="bold">
		Error: <%=error%>
	</div>
	<%}%>
	<%if(this.response){%>
	<%if(valid==1){%>
	<div style="color:green" class="bold">
		This URL is available!
	</div>
	<%}else{%>
	<%if(this.message){%>
	<div style="color:#f02" class="bold">
		<%=message%>
	</div>
	<%}else{%>
	<div style="color:#f02" class="bold">
		This URL has already been taken
	</div>
	<%}%>
	<%}%>
	<%}%>
</div>
@@@formbuilder_phone_input@@@
<div data-<%=template%>="<%=_tid%>">
	<div style="text-align: center;padding:10px 0px" class="phoneinput">
		<input type="tel" style="padding-top:5px;padding-bottom:5px;font-size:20px;border:1px solid #ccc;" class="truebox s-corner-all" placeholder="<%=name%>">
	</div>
</div>
@@@formbuilder_phone@@@
<div class="formelement forminput2" data-type="phone" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" <%if(form.group){%>data-group="<%=form.group%>"<%}%>>
