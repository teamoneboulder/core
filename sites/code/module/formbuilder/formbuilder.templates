@@@formbuilder_url@@@
<div class="formelement forminput2" data-type="url" data-key="<%=key%>" data-<%=template%>="<%=_tid%>" <%if(form.group){%>data-group="<%=form.group%>"<%}%>>
	<div class="webplaceholder"><%=name%> <%=$.fn.render({template:'formbuilder_moreinfo',data:{item:this},returntemplate:true})%></div>
	<div class="formbuilder_contentarea">
		<div style="padding:0px 10px">
			<%if(current){%>
			<div class="placeholdertitle notonweb"><%=name%></div>
			<div><%=current%></div>
			<%}else{%>
			<div class="placeholdercolor">
				<%=name%>
